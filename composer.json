{
  "name": "tombonez/multisite-url-fixer",
  "type": "wordpress-muplugin",
  "license": "MIT",
  "description":
    "Fixes WordPress issues with home and site URL on multisite when using Bedrock",
  "homepage": "https://github.com/tombonez/multisite-url-fixer",
  "keywords": ["wordpress"],
  "autoload": {
    "psr-4": {
      "Roots\\Bedrock\\": "src/"
    }
  },
  "require": {
    "php": ">=7.4.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0"
  },
  "scripts": {
    "test": ["phpcs --ignore=vendor --extensions=php --standard=PSR2 ."]
  }
}
